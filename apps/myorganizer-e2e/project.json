{
  "name": "myorganizer-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/myorganizer-e2e/src",
  "tags": [],
  "implicitDependencies": ["myorganizer"],
  "// targets": "to see all targets run: nx show project myorganizer-e2e --web",
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "options": {
        "config": "apps/myorganizer-e2e/playwright.config.ts",
        "project": ["chromium"]
      }
    },
    "e2e-firefox": {
      "executor": "@nx/playwright:playwright",
      "options": {
        "config": "apps/myorganizer-e2e/playwright.config.ts",
        "project": ["firefox"]
      }
    },
    "e2e-webkit": {
      "executor": "@nx/playwright:playwright",
      "options": {
        "config": "apps/myorganizer-e2e/playwright.config.ts",
        "project": ["webkit"]
      }
    },
    "e2e-all": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "{workspaceRoot}",
        "parallel": false,
        "commands": [
          "npx nx run myorganizer-e2e:e2e",
          "npx nx run myorganizer-e2e:e2e-firefox",
          "npx nx run myorganizer-e2e:e2e-webkit"
        ]
      }
    }
  }
}
