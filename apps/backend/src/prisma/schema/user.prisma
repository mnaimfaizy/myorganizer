model User {
    id                              String      @id @default(cuid()) 
    name                            String?
    first_name                      String
    last_name                       String
    phone                           String?
    email                           String      @unique
    password                        String
    reset_password_token            String?
    email_verification_timestamp    DateTime? 
    blacklisted_tokens              String[]

    encrypted_vault                  EncryptedVault?

    @@index([email])
}